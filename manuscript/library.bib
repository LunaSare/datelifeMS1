pmid = {18662398},
title = {{LOSITAN: a workbench to detect molecular adaptation based on a Fst-outlier method.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2515854{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {9},
year = {2008}
}
