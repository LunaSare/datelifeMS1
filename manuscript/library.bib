journal = {Ecology Letters},
keywords = {2002,2004,601,613,7,Mass effects,Metacommunity,Neutral model,Patch dynamics,Species sorting,ecology letters,however,mass effects,metacommunity,much of formal community,neutral model,patch dynamics,remarkably,species sorting},
