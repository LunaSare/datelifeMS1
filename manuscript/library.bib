pmid = {22217720},
title = {{Diversification and the adaptive radiation of the vangas of Madagascar}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-84859386818{\&}partnerID=40{\&}md5=b40482b02307176e7cd4d2f54d5c8df7},
