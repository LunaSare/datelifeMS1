month = {dec},
number = {3},
pages = {445--454},
pmid = {12450749},
title = {{Molecular phylogeny of 48 species of damselfishes (Perciformes: Pomacentridae) using 12S mtDNA sequences.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/12450749},
volume = {25},
