pages = {1658--1669},
title = {{Reproductive structures of Rhamnaceae from the Cerro del Pueblo (Late Cretaceous, Coahuila) and Coatzingo (Oligocene, Puebla) Formations, Mexico.}},
volume = {94},
year = {2007}
