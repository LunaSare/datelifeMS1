@article{Smith1988,
abstract = {The expansion of premodern humans into western and eastern$\backslash$nEurope ,40,000 years before the present led to the eventual$\backslash$nreplacement of the Neanderthals by modern humans ,28,000$\backslash$nyears ago$\backslash$n1$\backslash$n. Here we report the second mitochondrial DNA$\backslash$n(mtDNA) analysis of a Neanderthal, and the {\textregistered}rst such analysis$\backslash$non clearly dated Neanderthal remains. The specimen is from one$\backslash$nof the eastern-most Neanderthal populations, recovered from$\backslash$nMezmaiskaya Cave in the northern Caucasus$\backslash$n2$\backslash$n. Radiocarbon$\backslash$ndating estimated the specimen to be ,29,000 years old and$\backslash$ntherefore from one of the latest living Neanderthals$\backslash$n3$\backslash$n. The$\backslash$nsequence shows 3.48{\%} divergence from the Feldhofer$\backslash$nNeanderthal$\backslash$n4$\backslash$n. Phylogenetic analysis places the two Neanderthals$\backslash$nfrom the Caucasus and western Germany together in a clade that$\backslash$nis distinct from modern humans, suggesting that their mtDNA$\backslash$ntypes have not contributed to the modern human mtDNA pool.$\backslash$nComparison with modern populations provides no evidence for$\backslash$nthe multiregional hypothesis of modern human evolution.},
