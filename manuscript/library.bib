@article{Przeworski1998a,
abstract = {A hierarchical birth-death process is often used as a model for the$\backslash$ndiversification of species and higher taxa. We evaluate the validity of$\backslash$nthis null hypothesis by introducing a new measure: the distribution of$\backslash$nspecies within higher taxa. The proportion of monotypic taxa and the$\backslash$nnumber of species in the largest taxon generated by 1000 simulations are$\backslash$ncompared with extant mammalian data. The initial model is amended to$\backslash$ninclude two sampling patterns, polyphyletic origins as well as$\backslash$necological and genetic constraints on higher taxon origination rates.$\backslash$nSimulated results are extremely variable. For genera, they are found to$\backslash$npredict the true distribution of species quite well. For families,$\backslash$nhowever, the null hypothesis is rejected in all its forms. Simulated$\backslash$ndistributions have both too many monotypic taxa and too large a dominant$\backslash$ntaxon. The latter finding stands in contrast to previous claims.$\backslash$nInterestingly, although polyphyly and constraints slightly improve the$\backslash$nfit of the simulations to the data, sampling does not. Finally, Smith$\backslash$nand Patterson's dismissal of monotypic taxa is reviewed in light of this$\backslash$nmodel. We argue that while some apparently monotypic taxa are sampling$\backslash$nartifacts, this observation has no bearing on the true proportion of$\backslash$nmonotypic taxa.},
author = {Przeworski, M and Wall, J D},
isbn = {0094-8373},
issn = {0094-8373},
journal = {Paleobiology},
number = {4},
pages = {498--511},
title = {{An evaluation of a hierarchical branching process as a model for species diversification}},
volume = {24},
year = {1998}
}
@article{Dunn2009,
abstract = {There is growing evidence for a discontinuity between genomic and ecological divergence in several groups of bacteria. This evidence is difficult to reconcile with the traditional concept that ecologically divergent species maintain a cohesive gene pool isolated from other gene pools by barriers to homologous recombination (HR). There have been several innovative models of bacterial divergence that permit such discontinuity; we refer to these, collectively, as "mosaic genome concepts" (MGCs). These concepts remain a point of contention. Here, we undertake ail investigation among ecologically divergent lineages of genus Listeria, and report Our assessment of both niche-specific selection pressure and HR in their core genome. We find evidence of a mosaic Listeria core genome. Some core genes appear to have been free to recombine across ecologically divergent lineages or across named species. In contrast, other core genes have histories consistent with the expected organism relationships and have evolved under niche-specific selective pressures. The products of some of those genes can even be linked to metabolic phenotypes with ecological significance. This finding indicates a potentially strong connection between ecological divergence and core-genome evolution, even among lineages that also experience frequent recombination. Based oil these findings, we propose an expanded role for natural selection in core-genome evolution under the MGC.},
author = {Dunn, Katherine A. and Bielawski, Joseph P. and Ward, Todd J. and Urquhart, Caroline and Gu, Hong},
doi = {10.1093/molbev/msp176},
isbn = {0737-4038},
issn = {07374038},
journal = {Molecular Biology and Evolution},
keywords = {Core genome,Homologous recombination,Listeria,Metabolic network,Mosaic genome,Natural selection,Speciation,Species genome},
number = {11},
pages = {2605--2615},
pmid = {19666992},
title = {{Reconciling ecological and genomic divergence among lineages of listeria under an "extended mosaic genome concept"}},
volume = {26},
year = {2009}
}
