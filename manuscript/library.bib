@article{Tokuoka2006b,
abstract = {We present phylogenetic analyses of Malpighiales, which are poorly understood with respect to relationships within the order, using sequences from rbcL, atpB, matK and 18SrDNA from 103 genera in 23 families. From several independent and variously combined analyses, a four-gene analysis using all sequence data provided the best resolution, resulting in the single most parsimonious tree. In the Malpighiales [bootstrap support (BS) 100{\%}], more than eight major clades comprising a family or group of families successively diverged, but no clade containing more than six families received over 50{\%} BS. Instead, ten terminal clades that supported close relationships between and among families ({\textgreater}50{\%} BS) were obtained, between, for example, Balanopaceae and Chrysobalanaceae; Lacistemataceae and Salicaceae; and Phyllanthaceae and Picrodendraceae. The monophyly of Euphorbiaceae sens. str. were strongly supported (BS 100{\%}), but its sister group was unclear. Euphorbiaceae sens. str. comprised two basally diverging clades (BS 100{\%}): one leading to the Clutia group (Chaetocarpus, Clutia, Pera and Trigonopleura), and the other leading to the rest of the family. The latter shared a palisadal, instead of a tracheoidal exotegmen as a morphological synapomorphy. While both Acalyphoideae (excluding Dicoelia and the Clutia group) and Euphorbioideae are monophyletic, Crotonoideae were paraphyletic, requiring more comprehensive analyses.},
author = {Tokuoka, Toru and Tobe, Hiroshi},
doi = {10.1007/s10265-006-0025-4},
isbn = {0918-9440},
issn = {0918-9440},
journal = {Journal of Plant Research},
keywords = {18S,18S rDNA,18S: genetics,DNA,Euphorbiaceae,Euphorbiaceae: classification,Euphorbiaceae: embryology,Euphorbiaceae: genetics,Evolution,Genes,Malpighiales,Molecular,Molecular Sequence Data,Phylogeny,Plant,Plant: genetics,Plastids,Plastids: genetics,RNA,Ribosomal,Ribosomal: genetics,Ribulose-Bisphosphate Carboxylase,Ribulose-Bisphosphate Carboxylase: genetics,atpB,matK,rbcL},
month = {nov},
number = {6},
pages = {599--616},
pmid = {16937025},
title = {{Phylogenetic analyses of Malpighiales using plastid and nuclear DNA sequences, with particular reference to the embryology of Euphorbiaceae sens. str.}},
