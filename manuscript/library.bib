@article{Foote2007c,
abstract = {Occurrence data from the Paleobiology Database are used to analyze the waxing and waning of genera over time. Irrespective of whether we tabulate species richness, frequency of occurrence, geographic range, or other measures, the average rise and fall of genera is remarkably symmetrical. Genera tend already to be in a state of decline when they become extinct. Genera that last appear in the major mass extinction stages, however, are more frequently truncated while they are holding steady or even increasing. This need not imply that mass extinctions are qualitatively different from other events; it is consistent with the expected effects of simply increasing the magnitude of extinction. For reasons that are not completely clear, post-Paleozoic genera show less of a rise and fall on average and tend to be less symmetrical than do Paleozoic genera.},
author = {Foote, Michael},
doi = {10.1666/06084.1},
isbn = {0094-8373},
issn = {0094-8373},
journal = {Paleobiology},
number = {4},
pages = {517--529},
title = {{Symmetric waxing and waning of marine invertebrate genera}},
volume = {33},
year = {2007}
}
@article{Przeworski1998,
abstract = {A hierarchical birth-death process is often used as a model for the$\backslash$ndiversification of species and higher taxa. We evaluate the validity of$\backslash$nthis null hypothesis by introducing a new measure: the distribution of$\backslash$nspecies within higher taxa. The proportion of monotypic taxa and the$\backslash$nnumber of species in the largest taxon generated by 1000 simulations are$\backslash$ncompared with extant mammalian data. The initial model is amended to$\backslash$ninclude two sampling patterns, polyphyletic origins as well as$\backslash$necological and genetic constraints on higher taxon origination rates.$\backslash$nSimulated results are extremely variable. For genera, they are found to$\backslash$npredict the true distribution of species quite well. For families,$\backslash$nhowever, the null hypothesis is rejected in all its forms. Simulated$\backslash$ndistributions have both too many monotypic taxa and too large a dominant$\backslash$ntaxon. The latter finding stands in contrast to previous claims.$\backslash$nInterestingly, although polyphyly and constraints slightly improve the$\backslash$nfit of the simulations to the data, sampling does not. Finally, Smith$\backslash$nand Patterson's dismissal of monotypic taxa is reviewed in light of this$\backslash$nmodel. We argue that while some apparently monotypic taxa are sampling$\backslash$nartifacts, this observation has no bearing on the true proportion of$\backslash$nmonotypic taxa.},
author = {Przeworski, M and Wall, J D},
isbn = {0094-8373},
issn = {0094-8373},
journal = {Paleobiology},
number = {4},
pages = {498--511},
title = {{An evaluation of a hierarchical branching process as a model for species diversification}},
volume = {24},
year = {1998}
}
@article{Dial1989,
abstract = {Hollow curve distributions (HCDs) of taxonomic diversity are common in nature and are a result of assemblages being dominated by one or a few very diverse taxa. Models of random extinction and speciation can produce HCDs; however, the similarity between real and simulated HCDs has been examined previously only qualitatively. We compare quantitatively dominance observed in 85 real taxonomic assemblages to dominance predicted by five null models (1. Poisson; 2. Raup et al.'s [1973] simulation model; 3. Anderson and Anderson's [1975] simulation model; 4. simultaneous broken-stick; and 5. canonical lognormal distributions). Real assemblages were dominated to a significantly greater extent by one unit than predicted by all null models. This dominance is compounded as one proceeds down the taxonomic hierarchy. We demonstrate that overdominance is common whether phylogenetic or traditional classification schemes are used to assemble taxa. We propose that overdominance reflects real differences in the evolutionary success of units within an assemblage.},
author = {Dial, Kenneth P and Marzluff, John M},
doi = {10.1093/sysbio/38.1.26},
isbn = {00397989},
issn = {00397989},
journal = {Systematic Zoology},
number = {1},
pages = {26--37},
title = {{Nonrandom Diversification within Taxonomic Assemblages}},
volume = {38},
year = {1989}
}
@article{Clayton1972,
abstract = {The genus concept represents a subjective approach to a natural phenomenon. It is difficult to disentangle the natural element, which is itself very complex, and the subject is fraught with speculation, but there is at least some justification for the following tentative propositions. 1. The quasi-logarithmic distribution is remarkably constant, suggesting that evolutionary processes have operated in a reasonably uniform manner to produce a similar pattern of species groups in each family, and that these groups are recognizable as genera. 2. The Index of Diversity varies considerably, and may reflect a subjective dislike of large genera. 3. At least in the Gramineae new genera have been described at a relatively faster rate than new species have been discovered, and the level at which the genus is recognized has risen closer to that of the species.},
author = {Clayton, W D},
isbn = {00755974},
journal = {Kew Bulletin},
number = {2},
pages = {281--287},
title = {{Some Aspects of the Genus Concept}},
url = {http://www.jstor.org/stable/4109454},
volume = {27},
year = {1972}
}
