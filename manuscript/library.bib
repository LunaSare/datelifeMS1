@article{Derycke2013,
abstract = {Dispersal and gene flow determine connectivity among populations, and can be studied through population genetics and phylogeography. We here review the results of such a framework for free-living marine nematodes. Although field experiments have illustrated substantial dispersal in nematodes at ecological time scales, analysis of the genetic diversity illustrated the importance of priority effects, founder effects and genetic bottlenecks for population structuring between patches {\textless}1 km apart. In contrast, only little genetic structuring was observed within an estuary ({\textless}50 km), indicating that these small scale fluctuations in genetic differentiation are stabilized over deeper time scales through extensive gene flow. Interestingly, nematode species with contrasting life histories (extreme colonizers vs persisters) or with different habitat preferences (algae vs sediment) show similar, low genetic structuring. Finally, historical events have shaped the genetic pattern of marine nematodes and show that gene flow is restricted at large geographical scales. We also discuss the presence of substantial cryptic diversity in marine nematodes, and end with highlighting future important steps to further unravel nematode evolution and diversity.},
author = {Derycke, Sofie and Backeljau, Thierry and Moens, Tom},
doi = {10.1186/1742-9994-10-1},
issn = {1742-9994},
journal = {Frontiers in Zoology},
keywords = {Connectivity,Cryptic species,Dispersal,Gene flow,Life history,Marine nematodes,Phylogeography,Population genetics,connectivity,cryptic species,dispersal,gene flow,life history,marine nematodes,population genetics},
month = {jan},
number = {1},
pages = {1},
pmid = {23356547},
publisher = {Frontiers in Zoology},
title = {{Dispersal and gene flow in free-living marine nematodes.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3567977{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {10},
year = {2013}
}
