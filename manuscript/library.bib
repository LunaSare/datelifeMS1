keywords = {Africa,Argentina,Asteraceae,Asteraceae: anatomy {\&} histology,Asteraceae: classification,Asteraceae: genetics,Biological Evolution,Flowers,Flowers: anatomy {\&} histology,Flowers: genetics,Fossils,Phylogeny,Phylogeography,South America},
