issn = {1537-2197},
journal = {American Journal of Botany},
keywords = {Angiosperms,Biodiversity,Biological Evolution,Cyperaceae,Cyperaceae: genetics,Fossils,Genetic,Genetic Speciation,Models,Phylogeny,Species Specificity},
month = {dec},
number = {12},
pages = {2403--11},
pmid = {24249788},
title = {{Shifts in diversification rates and clade ages explain species richness in higher-level sedge taxa (Cyperaceae).}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24249788},
volume = {100},
year = {2013}
}
