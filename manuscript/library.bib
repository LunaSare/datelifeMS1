issn = {00225193},
journal = {Journal of Theoretical Biology},
keywords = {Coalescent,Distribution,Family size,Growing population},
number = {2},
pages = {245--256},
pmid = {19769992},
publisher = {Elsevier},
title = {{Universal features of surname distribution in a subsample of a growing population}},
