journal = {Journal of Evolutionary Biology},
keywords = {Altitude,Amphibian,Animals,Anura,Anura: anatomy {\&} histology,Anura: genetics,Biodiversity,Body Size,Diversification rates,Genetic Speciation,Latin America,Montane gradients,Phylogenetic comparative method,Phylogeny,Radiation,Skin,Skin: blood supply,Species richness,Terrarana,Tropical Climate},
month = {may},
