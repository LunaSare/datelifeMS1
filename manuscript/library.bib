pages = {5--17},
pmid = {21672718},
title = {{Are mountain passes higher in the tropics? Janzen's hypothesis revisited}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21672718},
volume = {46},
year = {2006}
}
