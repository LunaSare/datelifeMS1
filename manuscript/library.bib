journal = {Science},
keywords = {Animal,Animals,Biodiversity,Biological,Biological Evolution,Body Size,Body Weight,Conservation of Natural Resources,Ecosystem,Environment,Female,Homing Behavior,Humans,Mammals,Mammals: physiology,Models,Population Density,Population Dynamics,Pregnancy,Regression Analysis,Risk,Statistical,Weaning},
month = {aug},
