@article{Soltis2009a,
abstract = {Polyploidy has long been recognized as a major force in angiosperm evolution. Recent genomic investigations not only indicate that polyploidy is ubiquitous among angiosperms, but also suggest several ancient genome-doubling events. These include ancient whole genome duplication (WGD) events in basal angiosperm lineages, as well as a proposed paleohexaploid event that may have occurred close to the eudicot divergence. However, there is currently no evidence for WGD in Amborella, the putative sister species to other extant angiosperms. The question is no longer "What proportion of angiosperms are polyploid?", but "How many episodes of polyploidy characterize any given lineage?" New algorithms provide promise that ancestral genomes can be reconstructed for deep divergences (e.g., it may be possible to reconstruct the ancestral eudicot or even the ancestral angiosperm genome). Comparisons of diversification rates suggest that genome doubling may have led to a dramatic increase in species richness in several angiosperm lineages, including Poaceae, Solanaceae, Fabaceae, and Brassicaceae. However, additional genomic studies are needed to pinpoint the exact phylogenetic placement of the ancient polyploidy events within these lineages and to determine when novel genes resulting from polyploidy have enabled adaptive radiations.},
author = {Soltis, Douglas E and Albert, Victor A and Leebens-Mack, Jim and Bell, Charles D and Paterson, Andrew H and Zheng, Chunfang and Sankoff, David and Depamphilis, Claude W and Wall, P Kerr and Soltis, Pamela S},
doi = {10.3732/ajb.0800079},
issn = {0002-9122},
journal = {American Journal of Botany},
keywords = {2005,2006,2007,2008,angiosperm diversifi cation,as the papers in,doyle,e,friis et al,frohlich and chase,g,genome doubling,jansen et al,kim et al,polyploidy,recent reviews illustrate,tang et al,this issue and other,whole genome duplication},
month = {jan},
number = {1},
pages = {336--48},
pmid = {21628192},
title = {{Polyploidy and angiosperm diversification.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21628192},
volume = {96},
year = {2009}
}
@article{Marquinez2009a,
abstract = {Phylogenetic analyses of representative species from the five genera of Winteraceae (Drimys, Pseudowintera, Takhtajania, Tasmannia, and Zygogynum s.l.) were performed using ITS nuclear sequences and a combined data-set of ITS + psbA-trnH + rpS16 sequences (sampling of 30 and 15 species, respectively). Indel informativity using simple gap coding or gaps as a fifth character was examined in both data-sets. Parsimony and Bayesian analyses support the monophyly of Drimys, Tasmannia, and Zygogynum s.l., but do not support the monophyly of Belliolum, Zygogynum s.s., and Bubbia. Within Drimys, the combined data-set recovers two subclades. Divergence time estimates suggest that the splitting between Drimys and its sister clade (Pseudowintera + Zygogynum s.l.) occurred around the end of the Cretaceous; in contrast, the divergence between the two subclades within Drimys is more recent (15.5-18.5 MY) and coincides in time with the Andean uplift. Estimates suggest that the earliest divergences within Winteraceae could have predated the first events of Gondwana fragmentation. {\textcopyright} 2009 Elsevier Inc. All rights reserved.},
author = {Marqu{\'{i}}nez, Xavier and Lohmann, L{\'{u}}cia G. and Salatino, Maria L Faria and Salatino, Antonio and Gonz{\'{a}}lez, Favio},
