number = {3},
pages = {225--244},
title = {{The shape of evolution: systematic tree topology}},
url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1095-8312.1983.tb01874.x/abstract{\%}5Cnhttp://onlinelibrary.wiley.com/store/10.1111/j.1095-8312.1983.tb01874.x/asset/j.1095-8312.1983.tb01874.x.pdf?v=1{\&}t=hdg41fz8{\&}s=8ea0bc4b32ef39a3292ae2fa483c417411685524},
volume = {20},
