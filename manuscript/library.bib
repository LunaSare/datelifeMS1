journal = {American journal of botany},
keywords = {Biological,Colombia,Extinction,Fossils,Geography,Menispermaceae,Menispermaceae: anatomy {\&} histology,Menispermaceae: classification,Phylogeny,Seed Dispersal,Seed Dispersal: genetics,Seeds,Seeds: anatomy {\&} histology,Time Factors,Wyoming},
month = {dec},
