@article{Nyakatura2012,
abstract = {BACKGROUND: Although it has proven to be an important foundation for investigations of carnivoran ecology, biology and evolution, the complete species-level supertree for Carnivora of Bininda-Emonds et al. is showing its age. Additional, largely molecular sequence data are now available for many species and the advancement of computer technology means that many of the limitations of the original analysis can now be avoided. We therefore sought to provide an updated estimate of the phylogenetic relationships within all extant Carnivora, again using supertree analysis to be able to analyze as much of the global phylogenetic database for the group as possible. RESULTS: In total, 188 source trees were combined, representing 114 trees from the literature together with 74 newly constructed gene trees derived from nearly 45,000 bp of sequence data from GenBank. The greater availability of sequence data means that the new supertree is almost completely resolved and also better reflects current phylogenetic opinion (for example, supporting a monophyletic Mephitidae, Eupleridae and Prionodontidae; placing Nandinia binotata as sister to the remaining Feliformia). Following an initial rapid radiation, diversification rate analyses indicate a downturn in the net speciation rate within the past three million years as well as a possible increase some 18.0 million years ago; numerous diversification rate shifts within the order were also identified. CONCLUSIONS: Together, the two carnivore supertrees remain the only complete phylogenetic estimates for all extant species and the new supertree, like the old one, will form a key tool in helping us to further understand the biology of this charismatic group of carnivores.},
author = {Nyakatura, Katrin and Bininda-Emonds, Olaf R P},
doi = {10.1186/1741-7007-10-12},
issn = {1741-7007},
journal = {BMC Biology},
keywords = {Animals,Biological Evolution,Carnivora,Carnivora: anatomy {\&} histology,Carnivora: classification,Carnivora: genetics,Conservation of Natural Resources,DNA,Evolution,Molecular,Phylogeny,Sequence Analysis},
month = {jan},
number = {1},
pages = {12},
pmid = {22369503},
publisher = {BioMed Central Ltd},
title = {{Updating the evolutionary history of Carnivora (Mammalia): a new species-level supertree complete with divergence time estimates.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3307490{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {10},
