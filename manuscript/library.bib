pages = {3059--3066},
pmid = {12136088},
title = {{MAFFT: a novel method for rapid multiple sequence alignment based on fast Fourier transform.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=135756{\&}tool=pmcentrez{\&}rendertype=abstract},
