title = {{Testing macro-evolutionary models using incomplete molecular phylogenies.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1690817{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {267},
