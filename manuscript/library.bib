@article{Silvera2005a,
abstract = {Crassulacean acid metabolism (CAM) is one of three metabolic pathways found in vascular plants for the assimilation of carbon dioxide. In this study, we investigate the occurrence of CAM photosynthesis in 200 native orchid species from Panama and 14 non-native species by carbon isotopic composition ({\^{I}}´13C) and compare these values with nocturnal acid accumulation measured by titration in 173 species. Foliar {\^{I}}´13C showed a bimodal distribution with the majority of species exhibiting values of approximately -28{\^{a}}€° (typically associated with the C3 pathway), or -15{\^{a}}€° (strong CAM). Although thick leaves were related to {\^{I}}´13C values in the CAM range, some thin-leaved orchids were capable of CAM photosynthesis, as demonstrated by acid titration. We also found species with C3 isotopic values and significant acid accumulation at night. Of 128 species with {\^{I}}´13C more negative than -22{\^{a}}€°, 42 species showed nocturnal acid accumulation per unit fresh mass characteristic of weakly expressed CAM. These data suggest that among CAM orchids, there may be preferential selection for species to exhibit strong CAM or weak CAM, rather than intermediate metabolism. {\^{A}}{\textcopyright} CSIRO 2005.},
author = {Silvera, Katia and Santiago, Louis S. and Winter, Klaus},
doi = {10.1071/FP04179},
isbn = {1445-4408},
issn = {14454408},
journal = {Functional Plant Biology},
keywords = {Carbon stable isotope,Crassulacean acid metabolism,Evolution,Orchidaceae,Photosynthesis},
number = {5},
pages = {397--407},
title = {{Distribution of crassulacean acid metabolism in orchids of Panama: Evidence of selection for weak and strong modes}},
volume = {32},
year = {2005}
}
