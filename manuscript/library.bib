isbn = {1050516060},
issn = {1050-5164},
journal = {Annals of Applied Probability},
keywords = {Airy distribution,Catalan trees,Central limit theorem,Contraction method,Random phylogenetic trees,Shape statistics,Yule process,airy distribution,and phrases,catalan trees,central limit theorem,contraction method,random phylogenetic trees,shape statistics,yule process},
month = {nov},
