@article{Fritz2009,
abstract = {Whereas previous studies have investigated correlates of extinction risk either at global or regional scales, our study explicitly models regional effects of anthropogenic threats and biological traits across the globe. Using phylogenetic comparative methods with a newly-updated supertree of 5020 extant mammals, we investigate the impact of species traits on extinction risk within each WWF ecoregion. Our analyses reveal strong geographical variation in the influence of traits on risk: notably, larger species are at higher risk only in tropical regions. We then relate these patterns to current and recent-historical human impacts across ecoregions using spatial modelling. The body-mass results apparently reflect historical declines of large species outside the tropics due to large-scale land conversion. Narrow-ranged and rare species tend to be at high risk in areas of high current human impacts. The interactions we describe between biological traits and anthropogenic threats increase understanding of the processes determining extinction risk.},
author = {Fritz, Susanne a and Bininda-Emonds, Olaf R P and Purvis, Andy},
doi = {10.1111/j.1461-0248.2009.01307.x},
issn = {1461-0248},
journal = {Ecology Letters},
keywords = {Adaptation,Animals,Biological,Body Weight,Ecosystem,Extinction,Geography,Geography: statistics {\&} numerical data,Gestational Age,Inheritance Patterns,Mammals,Mammals: classification,Models,Phylogeny,Physiological,Population Density,Population Dynamics,Risk Factors,Tropical Climate,Weaning},
month = {jun},
number = {6},
pages = {538--49},
pmid = {19392714},
title = {{Geographical variation in predictors of mammalian extinction risk: big is bad, but only in the tropics.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19392714},
volume = {12},
year = {2009}
}
@article{Schluter2001,
abstract = {The ecological hypothesis of speciation is that reproductive isolation evolves ultimately as a consequence of divergent natural selection on traits between environments. Ecological speciation is general and might occur in allopatry or sympatry, involve many agents of natural selection,and result from a combination of adaptive processes.The main difficulty of the ecological hypothesis has been the scarcity of examples from nature, but several potential cases have recently emerged. I review the mechanisms that give rise to new species by divergent selection,compare ecological speciation with its alternatives,summarize recent tests in nature, and highlight areas requiring research.},
author = {Schluter, Dolph},
doi = {10.1016/S0169-5347(01)02198-X},
issn = {01695347},
journal = {Trends in Ecology {\&} Evolution},
number = {7},
