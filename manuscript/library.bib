keywords = {184,4,no,the american naturalist},
number = {4},
pages = {447--455},
pmid = {25226180},
title = {{On age and species richness of higher taxa.}},
url = {http://www.jstor.org/stable/10.1086/677676{\%}5Cnhttp://www.jstor.org/stable/pdfplus/10.1086/677676.pdf?acceptTC=true},
volume = {184},
year = {2014}
}
@article{Ezard2011,
abstract = {Ecological change provokes speciation and extinction, but our knowledge of the interplay among the biotic and abiotic drivers of macroevolution remains limited. Using the unparalleled fossil record of Cenozoic macroperforate planktonic foraminifera, we demonstrate that macroevolutionary dynamics depend on the interaction between species' ecology and the changing climate. This interplay drives diversification but differs between speciation probability and extinction risk: Speciation was more strongly shaped by diversity dependence than by climate change, whereas the reverse was true for extinction. Crucially, no single ecology was optimal in all environments, and species with distinct ecologies had significantly different probabilities of speciation and extinction. The ensuing macroevolutionary dynamics depend fundamentally on the ecological structure of species' assemblages.},
author = {Ezard, Thomas H G and Aze, Tracy and Pearson, Paul N and Purvis, Andy},
doi = {10.1126/science.1203060},
isbn = {1095-9203 (Electronic)$\backslash$r0036-8075 (Linking)},
issn = {0036-8075},
journal = {Science},
number = {april},
pages = {349--351},
pmid = {21493859},
title = {{Interplay Between Changing Climate and Species' Ecology Drives Macroevolutionary Dynamics}},
url = {http://www.sciencemag.org/content/332/6027/349.abstract{\%}5Cnhttp://www.sciencemag.org/content/332/6027/349.full.pdf},
volume = {332},
year = {2011}
}
@article{Hallinan2012,
abstract = {Much recent research has investigated the effect that different time variable birth-death processes have on the distribution of branching times in phylogenies of extant taxa. Previous work has shown how to calculate the distributions of number of lineages and branching times for a reconstructed constant rate birth-death process that started with one or two reconstructed lineages at some time in the past or ended with some number of lineages in the present. Here I expand that work to include any time variable birth-death process that starts with any number of reconstructed lineages and/or ends with any number of reconstructed lineages at any time, and I calculate a number of distributions under that process. I also explore the discrete time birth-death process which operates as an efficient and accurate numerical solution to any time-variable birth-death process and allows for the analytical incorporation of sampling and mass extinctions. I describe how these distributions can be used to compare different time variable models using maximum likelihood analysis, and I show how to simulate random trees under any of these models. I also introduce two visual methods for evaluating different time variable birth-death processes; these methods illustrate the shape of distributions for the number of lineages and waiting times by plotting them over time.},
author = {Hallinan, Nathaniel},
doi = {10.1016/j.jtbi.2012.01.041},
issn = {1095-8541},
journal = {Journal of Theoretical Biology},
keywords = {Algorithms,Animals,Biological,Birth Rate,Extinction,Markov Chains,Mortality,Paleontology,Paleontology: methods,Phylogeny},
month = {may},
pages = {265--76},
pmid = {22326474},
publisher = {Elsevier},
title = {{The generalized time variable reconstructed birth-death process.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22326474},
volume = {300},
year = {2012}
}
@article{Arcila2015a,
abstract = {Time-calibrated phylogenies based on molecular data provide a framework for comparative studies. Calibration methods to combine fossil information with molecular phylogenies are, however, under active development, often generating disagreement about the best way to incorporate paleontological data into these analyses. This study provides an empirical comparison of the most widely used approach based on node-dating priors for relaxed clocks implemented in the programs BEAST and MrBayes, with two recently proposed improvements: one using a new fossilized birth-death process model for node dating (implemented in the program DPPDiv), and the other using a total-evidence or tip-dating method (implemented in MrBayes and BEAST). These methods are applied herein to tetraodontiform fishes, a diverse group of living and extinct taxa that features one of the most extensive fossil records among teleosts. Previous estimates of time-calibrated phylogenies of tetraodontiforms using node-dating methods reported disparate estimates for their age of origin, ranging from the late Jurassic to the early Paleocene (ca. 150-59Ma). We analyzed a comprehensive dataset with 16 loci and 210 morphological characters, including 131 taxa (95 extant and 36 fossil species) representing all families of fossil and extant tetraodontiforms, under different molecular clock calibration approaches. Results from node-dating methods produced consistently younger ages than the tip-dating approaches. The older ages inferred by tip dating imply an unlikely early-late Jurassic (ca. 185-119Ma) origin for this order and the existence of extended ghost lineages in their fossil record. Node-based methods, by contrast, produce time estimates that are more consistent with the stratigraphic record, suggesting a late Cretaceous (ca. 86-96Ma) origin. We show that the precision of clade age estimates using tip dating increases with the number of fossils analyzed and with the proximity of fossil taxa to the node under assessment. This study suggests that current implementations of tip dating may overestimate ages of divergence in calibrated phylogenies. It also provides a comprehensive phylogenetic framework for tetraodontiform systematics and future comparative studies.},
author = {Arcila, Dahiana and {Alexander Pyron}, R and Tyler, James C and Ort{\'{i}}, Guillermo and Betancur-R, Ricardo},
doi = {10.1016/j.ympev.2014.10.011},
isbn = {1095-9513 (Electronic)$\backslash$r1055-7903 (Linking)},
issn = {1095-9513},
journal = {Molecular Phylogenetics and Evolution},
keywords = {Animals,Bayes Theorem,Biological Evolution,Calibration,Classification,Classification: methods,DNA,Fossils,Phylogeny,Sequence Analysis,Tetraodontiformes,Tetraodontiformes: classification},
pages = {131--145},
pmid = {25462998},
publisher = {Elsevier Inc.},
title = {{An evaluation of fossil tip-dating versus node-age calibrations in tetraodontiform fishes (Teleostei: Percomorphaceae).}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25462998},
volume = {82},
year = {2015}
}
@article{Maddison2015,
abstract = {When comparative biologists observe that animal$\backslash$nspecies living in caves also tend to have reduced eyes,$\backslash$nthey may see such correlation as evidence that the$\backslash$ntraits are adaptively or functionally linked: for instance,$\backslash$nselection to maintain eye function is relaxed when light$\backslash$nis unavailable. Such cross-species correlations cannot$\backslash$ngive definitive tests of evolutionary mechanisms, but$\backslash$nnonetheless offer important insights into biological$\backslash$nrelationshipsamongtraits in realms as diverse as ecology$\backslash$n(e.g., Paradis et al. 1998; Purvis et al. 2000) and genomics$\backslash$n(e.g., von Mering et al. 2002; Barker and Pagel 2005).$\backslash$nHowever, the last few decades have taught us that$\backslash$namong-species correlative tests should take into account$\backslash$nevolutionary relationships (Felsenstein 1985; Ridley$\backslash$n1989; Harvey and Pagel 1991). If phylogeny is not$\backslash$ntaken into account, an interpreted correlation may$\backslash$nhave a trivial explanation different from the biological$\backslash$nrelationship we claim. There is a correlation among$\backslash$nspecies in the distribution of fur and bones in the middle$\backslash$near--species with fur also have three bones in the middle$\backslash$near, and vice versa. These two traits are characteristics$\backslash$nof mammals, and absent outside the mammals. Using$\backslash$ntheir shared distribution as evidence of an interesting$\backslash$nbiological relationship between fur and middle ear bones$\backslash$nwould be considered a mistake, however, for reasons$\backslash$nunderstood long ago by Darwin (1872):$\backslash$nWe may often falsely attribute to correlated$\backslash$nvariation structures which are common to$\backslash$nwhole groups of species, and which in truth$\backslash$nare simply due to inheritance; for an ancient$\backslash$nprogenitor may have acquired through$\backslash$nnatural selection some one modification$\backslash$nin structure, and, after thousands of$\backslash$ngenerations, some other and independent$\backslash$nmodification; and these two modifications,$\backslash$nhaving been transmitted to a whole group$\backslash$nof descendants with diverse habits, would$\backslash$nnaturally be thought to be in some necessary$\backslash$nmanner correlated.$\backslash$nIn modern vocabulary,wewould say that the thousands$\backslash$nof species of mammals are not statistically independent$\backslash$nbecause of their shared history (Felsenstein 1985; Ridley$\backslash$n1989), instead being pseudoreplicates. Many methods$\backslash$nhave been developed to avoid pseudoreplication while$\backslash$nassessing cross-species correlations by accounting$\backslash$nfor phylogenetic relationships (e.g., Felsenstein 1985;$\backslash$nMaddison 1990; Harvey and Pagel 1991; Garland et al.$\backslash$n1992;Pagel 1994;ReadandNee 1995;MartinsandHansen$\backslash$n1997; Huelsenbeck et al. 2003; Hadfield and Nakagawa$\backslash$n2010). These phylogenetically aware correlation methods$\backslash$nhave been applied with enthusiasm, although there have$\backslash$nbeen skeptics questioning their need (Westoby 1999) and$\backslash$ncautions expressed over their use (Ricklefs and Starck$\backslash$n1996; Freckleton 2009).$\backslash$nNot all is healthy with the paradigm, however.$\backslash$nFor categorical characters a special concern has been$\backslash$nraised: commonly used and well-respected methods$\backslash$n(e.g., Pagel 1994), do not eliminate pseudoreplication,$\backslash$nas they are susceptible to an effect from a single$\backslash$nevolutionary event (Maddison 1990, 2000; Read and Nee$\backslash$n1995; Ridley and Grafen 1996; Grafen and Ridley 1997).$\backslash$nAs a result, a significant statistical association between$\backslash$ntraits inferred by these methods can mean very little$\backslash$nin some circumstances, misleading biologists about the$\backslash$nrelationship between traits.$\backslash$nAlthough this concern has been raised, there has been$\backslash$nlittle effect on the practice of comparative studies, we$\backslash$nthink because the issue has not been well understood.$\backslash$nWe do not, alas, have a solution. Our purpose here is$\backslash$nto explore the issue in depth in part to bring caution$\backslash$nto comparative studies, in part to characterize how$\backslash$nour methods should behave in hopes of provoking$\backslash$nappropriate solutions.},
author = {Maddison, Wayne P and FitzJohn, Richard G},
doi = {10.1093/sysbio/syu070},
isbn = {1076-836X (Electronic)$\backslash$r1063-5157 (Linking)},
issn = {1063-5157},
journal = {Systematic Biology},
number = {1},
pages = {127--136},
pmid = {25209222},
title = {{The Unsolved Challenge to Phylogenetic Correlation Tests for Categorical Characters}},
url = {http://sysbio.oxfordjournals.org/content/64/1/127.short{\%}5Cnhttp://sysbio.oxfordjournals.org/content/64/1/127},
volume = {64},
year = {2015}
}
@article{Stadler2013a,
abstract = {Speciation and extinction rates can be estimated from molecular phylogenies. Recently, a number of methods have been published showing that these rates can be estimated even if the phylogeny is incomplete, that is, if not all extant species are included. We show that the accuracy of such methods strongly depends on making the correct assumptions about how the sampling process was performed. We focus on phylogenies that are incomplete because some subclades (e.g., genera and families) are each represented as a single lineage. We show that previous methods implicitly assumed that such subclades are defined by randomly (or in an extreme deterministic way) choosing the edges that define the subclades from the complete species phylogeny. We show that these methods produce biased results if higher taxa are defined in a different manner. We introduce strict higher level phylogenies where subclades are defined so that the phylogeny is fully resolved from its origin to time xcut, and fully unresolved thereafter, so that for all subclades, stem age {\textgreater} xcut {\textgreater} crown age. We present estimates of speciation and extinction rates from a phylogeny of birds in which this subclade definition was applied. However, for most higher level phylogenies in the literature, it is unclear how higher taxa were defined, but often such phylogenies can be easily transformed into strict higher level phylogenies, as we illustrate by estimating speciation and extinction rates from a near-complete but only partly resolved species-level phylogeny of mammals. The accuracy of our methods is verified using simulations. [Birth–death process; higher taxa; macroevolution; phylogenetics.]},
author = {Stadler, Tanja and Bokma, Folmer},
doi = {10.1093/sysbio/sys087},
isbn = {1063-5157},
issn = {10635157},
journal = {Systematic Biology},
keywords = {Animals,Biological,Birds,Classification,Classification: methods,Computer Simulation,Extinction,Genetic Speciation,Mammals,Models,Phylogeny},
month = {mar},
number = {2},
pages = {220--230},
pmid = {23103589},
title = {{Estimating speciation and extinction rates for phylogenies of higher taxa}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23103589},
volume = {62},
year = {2013}
}
