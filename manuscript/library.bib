keywords = {Amphibian chytrid,Amphibian declines,Amphibians,Amphibians: microbiology,Animals,Beta diversity,Biodiversity,Biological,Biotic homogenization,Chytridiomycosis,Chytridiomycota,Chytridiomycota: physiology,Extinction,Mycoses,Mycoses: veterinary,Phylogeny,Population Density,Scale-dependence,Similarity,Wildlife disease},
month = {oct},
number = {10},
pages = {1069--1078},
pmid = {19694784},
title = {{Selecting for extinction: nonrandom disease-associated extinction homogenizes amphibian biotas.}},
