pages = {5974},
pmid = {25099137},
title = {{Epiphytic leafy liverworts diversified in angiosperm-dominated forests.}},
url = {http://www.nature.com/srep/2014/140807/srep05974/full/srep05974.html},
volume = {4},
year = {2014}
