isbn = {0026-6493},
issn = {00266493},
journal = {Annals of the Missouri Botanical Garden},
keywords = {anemopsis,angiosperm phylogeny,floral ontogeny,flower development,genomes,gymnotheca,houttuynia,houttuynia-cordata,inflorescence,mitochondrial,morphology,multigene data,phylogeny,piperaceae,rbcl,ribosomal DNA-sequences,saururaceae,saururus,utility},
