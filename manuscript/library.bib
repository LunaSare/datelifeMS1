pmid = {24695424},
title = {{The evolutionary reality of higher taxa in mammals.}},
url = {http://rspb.royalsocietypublishing.org/cgi/doi/10.1098/rspb.2013.2750{\%}5Cnhttp://www.ncbi.nlm.nih.gov/pubmed/24695424},
