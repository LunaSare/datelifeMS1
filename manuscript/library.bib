journal = {American Journal of Botany},
number = {2},
pages = {153--176},
title = {{Angiosperm flowers and tricolpate pollen of buxaceous affinity from the Potomac Group (mid-Cretaceous) of eastern North America}},
url = {http://www.jstor.org/stable/2445239},
volume = {3},
year = {1991}
}
